{
  "name": "@lomray/react-mobx-manager",
  "version": "1.0.0",
  "description": "This package provides Mobx stores manager for react.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./*": "./lib/*.js"
  },
  "typesVersions": {
    "*": {
      "./*": [
        "lib/*",
        "lib/storages/*"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lomray-Software/react-mobx-manager"
  },
  "keywords": [
    "react",
    "types",
    "typescript",
    "lomray",
    "javascript",
    "mobx",
    "store",
    "manager",
    "stores",
    "state"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Mikhail Yarmaliuk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lomray-Software/react-mobx-manager/issues"
  },
  "homepage": "https://github.com/Lomray-Software/react-mobx-manager",
  "scripts": {
    "build": "rollup -c",
    "build:dev": "rollup -c --environment BUILD:development",
    "build:watch": "rollup -c -w --environment BUILD:development",
    "lint:check": "eslint --ext \".ts,.tsx\" \"src/**/*.{ts,tsx,*.ts,*tsx}\"",
    "lint:format": "eslint --fix --ext \".ts,.tsx\" \"src/**/*.{ts,tsx,*.ts,*tsx}\"",
    "ts:check": "tsc --project ./tsconfig.checks.json --skipLibCheck --noemit",
    "test": "mocha --no-warnings  __tests__ --recursive --unhandled-rejections=strict --exit",
    "test:watch": "mocha --no-warnings  __tests__ --recursive --unhandled-rejections=strict",
    "prepare": "husky install"
  },
  "dependencies": {
    "@lomray/consistent-suspense": "^1.3.1",
    "@lomray/event-manager": "^2.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@lomray/eslint-config-react": "^3.0.0",
    "@lomray/prettier-config": "^1.2.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/chai": "^4.3.11",
    "@types/hoist-non-react-statics": "^3.3.5",
    "@types/lodash": "^4.14.202",
    "@types/mocha": "^10.0.6",
    "@types/react": "^18.2.42",
    "@types/sinon": "^17.0.2",
    "@types/sinon-chai": "^3.2.12",
    "c8": "^8.0.1",
    "chai": "^4.3.10",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "mocha": "^10.2.0",
    "prettier": "^3.1.0",
    "rollup": "2.79.1",
    "rollup-plugin-ts": "^3.4.5",
    "semantic-release": "^21.1.2",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "ttypescript": "^1.5.15",
    "typescript": "^4.9.5",
    "vite": "^5.0.6"
  },
  "peerDependencies": {
    "hoist-non-react-statics": ">=3.3.2",
    "lodash": ">=4.17.21",
    "mobx": ">=6.9.0",
    "mobx-react-lite": "^3 || ^4",
    "react": "^18 || ^17"
  }
}
